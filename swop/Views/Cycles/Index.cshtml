<head>
    @if ((bool)Session["Logged"] == false)
    {
        <meta http-equiv="refresh" content="0; url = http://localhost:54598/Home/Index" />
    }
    else
    {
        <meta http-equiv="refresh" content="0; url = http://localhost:54598/HomePage/Index" />

    }
</head>
@model IEnumerable<swop.Models.Cycle>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Start)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.End)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Start)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.End)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CycleId }) |
            @Html.ActionLink("Details", "Details", new { id=item.CycleId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.CycleId })
        </td>
    </tr>
}

</table>
